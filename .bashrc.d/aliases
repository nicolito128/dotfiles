# Sample aliases
alias ls='ls -hF --color=auto'
alias la='ls -ahF --color=auto'
alias dir='ls -a --color=auto --format=long'
alias grep='grep --color=auto'

# Fastfetch
alias ffetch='fastfetch'

# Toolbox
alias tbx='toolbox'

# Distrobox
alias dbx='distrobox'

# Containers
alias inpkg='distrobox-enter pkg -e tmux'
